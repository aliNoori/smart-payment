{
    "name": "amedev/smart-payment",
    "description": "A smart and extensible Laravel payment system",
    "type": "library",
    "version": "1.0.3",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Noori",
            "email": "matineali@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SmartPayment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartPayment\\Providers\\SmartPaymentServiceProvider"
            ]
        }
    },
    "scripts": {
        "release": [
            "@php release.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
